.btn-flip-down {
  --flip-button-height: 40px;
  height: var(--flip-button-height);
  color: transparent;
  border: none;
  border-radius: 0;
  perspective: 500px;
  overflow: visible;
  transition: 0.3s;
}

.btn-flip-down:hover {
  color: transparent;
  background: transparent;
}

.btn-flip-down .front,
.btn-flip-down .back {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}

.btn-flip-down .front {
  background: var(--btn-color);
  color: white;
  transition: 0.3s;
  transform-origin: center center calc(var(--flip-button-height) / -2);
}

.btn-flip-down .back {
  color: white;
  background: var(--btn-color);
  transform: rotateX(88deg);
  transform-origin: center center calc(var(--flip-button-height) / -2);
  transition: 0.3s;
}

.btn-flip-down:hover .front {
  transform: rotateX(-90deg);
}

.btn-flip-down:hover .back {
  transform: rotateX(0deg);
}

.btn-marquee {
  font-weight: 600;
  border: none;
  border-radius: 1.5rem;
  transition: 0.3s;
